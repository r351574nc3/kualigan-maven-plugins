<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="kfs"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <requiredProperties>
        <requiredProperty key="kfsPrototypeGroupId">
            <defaultValue>org.kuali.kfs</defaultValue>
        </requiredProperty>
        <requiredProperty key="kfsPrototypeArtifactId">
            <defaultValue>kfs</defaultValue>
        </requiredProperty>
        <requiredProperty key="kfsPrototypeVersion">
            <defaultValue>5.0</defaultValue>
        </requiredProperty>
    </requiredProperties>
  <modules>
    <fileSets>
        <fileSet encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>pom.xml</include>
          </includes>
        </fileSet>
    </fileSets>
    <module id="${rootArtifactId}-it" dir="it" name="${rootArtifactId}-it">
        <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/resources</directory>
          <includes>
            <include>**/*.txt</include>
            <include>**/*.xml</include>
            <include>**/*.properties</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-config" dir="config" name="${rootArtifactId}-config">
      <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/filters</directory>
          <includes>
            <include>**/*.properties</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/resources</directory>
          <includes>
            <include>**/*.txt</include>
            <include>**/*.xml</include>
            <include>**/*.properties</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/main/resources</directory>
          <includes>
            <include>**/*.data</include>
            <include>**/*.recon</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-core" dir="core" name="${rootArtifactId}-core">
      <fileSets>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/resources</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/main/resources</directory>
          <includes>
            <include>**/*.ks</include>
            <include>**/*.ldif</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-web" dir="web" name="${rootArtifactId}-web">
      <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/filters</directory>
          <includes>
            <include>**/*.properties</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/webapp</directory>
          <includes>
            <include>**/*.jsp</include>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
        <fileSet filtered="false" packaged="false" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/resources</directory>
          <includes>
            <include>**/*.xml</include>
            <include>**/*.properties</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/main/webapp</directory>
          <includes>
            <include>**/*.jpg</include>
            <include>**/*.inc</include>
            <include>**/*.png</include>
            <include>**/*.tag</include>
            <include>**/*.js</include>
            <include>**/*.tld</include>
            <include>**/*.gif</include>
            <include>**/*.css</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/main/resources</directory>
          <includes>
            <include>**/*.xsd</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
  </modules>
</archetype-descriptor>
