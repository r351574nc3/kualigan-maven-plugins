<project xmlns="http://maven.apache.org/DECORATION/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.1.0 http://maven.apache.org/xsd/decoration-1.1.0.xsd"
  name="Liquibase Copy Plugin">
    <bannerLeft>
        <name>Kuali</name>
        <src>images/kuali_commercial.png</src>
        <href>http://www.kuali.org</href>
    </bannerLeft>

    <skin>
      <groupId>org.apache.maven.skins</groupId>
      <artifactId>maven-fluido-skin</artifactId>
      <version>1.3.1</version>
    </skin>

    
    <custom>
      <fluidoSkin>
	<googlePlusOne />
	<gitHub>
          <projectId>r351574nc3/kualigan-maven-plugins</projectId>
          <ribbonOrientation>right</ribbonOrientation>
          <ribbonColor>red</ribbonColor>
	</gitHub>
	<topBarEnabled>true</topBarEnabled>
	<sideBarEnabled>false</sideBarEnabled>
      </fluidoSkin>
    </custom>


    <body>
        <menu name="Kualigan Maven Plugins">
          <item name="Overview" href="index.html"/>
        </menu>        
        <menu ref="reports" />
    </body>
    <poweredBy>
      <logo name="Maven" href="http://maven.apache.org/"
            img="http://maven.apache.org/images/logos/maven-feather.png"/>
    </poweredBy>
</project>
